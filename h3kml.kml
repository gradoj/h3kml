<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://www.opengis.net/kml/2.2">
  <Folder>
    <name>H3 Hex Grid</name>
    <visibility>0</visibility>
    <open>0</open>
    <description>H3 Hex Grid</description>
    <NetworkLink>
      <name>H3 Poly Calculator</name>
      <visibility>0</visibility>
      <open>0</open>
      <description>The view-based refresh allows the remote server to calculate
         the center of your screen and return hex grids.</description>
      <refreshVisibility>0</refreshVisibility>
      <flyToView>0</flyToView>
      <Link>
        <href>http://localhost:8000/</href>
        <refreshInterval>10</refreshInterval>
        <viewRefreshMode>onStop</viewRefreshMode>
        <viewRefreshTime>0.1</viewRefreshTime>
        <viewFormat>BBOX=[bboxWest],[bboxSouth],[bboxEast],[bboxNorth];CAMERA=\
      [cameraAlt]</viewFormat>
      </Link>
    </NetworkLink>
  </Folder>
</kml>